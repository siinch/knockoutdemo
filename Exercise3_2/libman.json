{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.min.css"
      ]
    },
    {
      "library": "requirejs@2.3.6",
      "destination": "wwwroot/lib/requirejs/",
      "files": [
        "require.js"
      ]
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.js",
        "dist/jquery.min.js"
      ]
    },
    {
      "library": "knockout@3.5.1",
      "destination": "wwwroot/lib/knockout/",
      "files": [
        "build/output/knockout-latest.debug.js",
        "build/output/knockout-latest.js",
        "build/types/knockout.d.ts"
      ]
    },
    {
      "library": "requirejs-text@2.0.15",
      "destination": "wwwroot/lib/requirejs-text/",
      "files": [
        "text.js"
      ]
    }
  ]
}